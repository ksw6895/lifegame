{
    "version": 2,
    "builds": [
        {
            "src": "backend/",
            "use": "@vercel/python"
        },
        {
            "src": "public/index.html",
            "use": "@vercel/static"
        },
        {
            "src": "public/style.css",
            "use": "@vercel/static"
        },
        {
            "src": "public/script.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/main.py"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/index.html",
            "dest": "/public/index.html"
        },
        {
            "src": "/style.css",
            "dest": "/public/style.css"
        },
        {
            "src": "/script.js",
            "dest": "/public/script.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}
